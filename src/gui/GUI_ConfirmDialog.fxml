<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="372.0" prefWidth="478.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.GUI_ConfirmDialogController">

    <children>
        <Label fx:id="informationMessageLabel" layoutY="70.0"
               AnchorPane.leftAnchor="40.0" AnchorPane.rightAnchor="40.0"
               alignment="CENTER" textAlignment="CENTER" wrapText="true"/>
        <Label fx:id="confirmMessageLabel" layoutY="230.0"
               AnchorPane.leftAnchor="40.0" AnchorPane.rightAnchor="40.0"
               alignment="CENTER" textAlignment="CENTER" wrapText="true"/>
        <HBox alignment="CENTER" layoutX="154.0" layoutY="307.0" spacing="40.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
               <Button fx:id="confirmButton" onAction="#handleOk" style="-fx-background-color: #2772d3;" text="Confirmar" textFill="WHITE" />
               <Button fx:id="cancelButton" onAction="#handleCancel" style="-fx-background-color: #2772d3;" text="Cancelar" textFill="WHITE" />
         </children>
        </HBox>
    </children>

</AnchorPane>
